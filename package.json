{
  "name": "snaphy-paddle",
  "description": "The Paddle.com Node.js SDK",
  "version": "1.0.1",
  "main": "sdk.js",
  "author": "Robins Gupta <robins@snaphy.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Snaphy-Automata/paddle-sdk.git"
  },
  "license": "MIT",
  "keywords": [
    "paddle",
    "sdk",
    "api",
    "payments",
    "subscriptions"
  ],
  "engines": {
    "node": ">=8.5.0"
  },
  "files": [
    "sdk.js",
    "lib/"
  ],
  "scripts": {
    "docs": "jsdoc2md --partial docs/main.hbs --helper docs/helpers.js --files sdk.js > Documentation.md",
    "lint": "eslint . --cache --fix --ignore-pattern '!.eslintrc.js'",
    "pretty": "prettier --write --list-different \"**/*.js\"",
    "release": "yarn docs && git add Documentation.md && standard-version -a",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test": "yarn test:jest",
    "pretest": "yarn lint"
  },
  "dependencies": {
    "got": "^9.6.0"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "jsdoc-to-markdown": "^5.0.0",
    "lint-staged": "^8.2.1",
    "nock": "^10.0.6",
    "prettier": "^1.18.2",
    "standard-version": "^6.0.1",
    "validate-commit-msg": "^2.14.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": "lint-staged"
    }
  }
}
